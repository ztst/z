{% if not videos is empty %}
  <ul class="videos-with-questions-list">
    {% set isFirst = true %}
    {% for video in videos %}
      <li class="{% if isFirst %}first{% endif %} open-video-question-link" id="openVideoQuestionLink{{ video.videoId }}">
        <div class="video-with-questions-list-item">
          <a
            href="{{ path('show_video', {
            'subjectName': video.getSubject.getUrlName,
            'class': video.getGrade,
            'videoName': video.getUrlName
            }) }}"
            class="">
            {{ video.name }}
          </a>
          <div class="clear"></div>
          <p class="video-breadcrumbs">
            {{ video.grade }} класс > {{ video.subject.name }} > {{ video.chapter.name }}
          </p>
        </div>
        <div class="right-count-column">
          <span class="list-count-container user-questions-count">{{ count_video_questions(video) }}</span>
        </div>
      </li>
      {% set isFirst = false %}
    {% endfor %}
  </ul>
{% endif %}