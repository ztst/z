{% if users %}
  {% for user in users %}
    <li class="" id="userContainer{{ user.UserId }}">
      <a id="openUserProfile{{ user.userId }}" class="show-user-profile-link" href="javascript:void(0);">
        <img class="media-object"
             width="50"
             height="50"
             src="{{ user_photo_small_url(user) }}"
             alt="Здесь будет аватар"/>
        <span class="user-name">{{ user }}</span>
        <a href="javascript:void(0);" title="" class="btn btn-primary send-founded-user-message-button">Написать</a>
        {% if user.region %}<span>{{ user.region.regionName }}</span>{% endif %}
        {{ search_user_role(user) }}
      </a>
    </li>
  {% endfor %}
{% else %}
  <li><h5>Не найдено</h5></li>
{% endif %}